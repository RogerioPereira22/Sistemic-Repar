{
  "name": "payload-starter-typescript",
  "description": "Blank template - no collections",
  "version": "1.0.0",
  "main": "dist/server.js",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "start": "node dist/server",
    "start:dev": "nodemon dist/server.js",
    "dev": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts",
    "build:payload": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts payload build",
    "build:server": "tsc",
    "build": "yarn copyfiles && yarn build:payload && build: tsc -p tsconfig.json",
    "serve": "cross-env PAYLOAD_CONFIG_PATH=dist/payload.config.js NODE_ENV=production node dist/server.js",
    "copyfiles": "copyfiles -u 1 \"src/**/*.{html,css,scss,ttf,woff,woff2,eot,svg,jpg,png}\" dist/",
    "generate:types": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts payload generate:types",
    "generate:graphQLSchema": "PAYLOAD_CONFIG_PATH=src/payload.config.ts payload generate:graphQLSchema"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.192.0",
    "@payloadcms/plugin-cloud-storage": "^1.0.12",
    "@payloadcms/plugin-form-builder": "^1.0.13",
    "@payloadcms/plugin-nested-docs": "^1.0.4",
    "@payloadcms/plugin-seo": "^1.0.9",
    "cjs": "^0.0.11",
    "cjs-loader": "^0.1.0",
    "cjs-module": "^1.5.0",
    "cross-env": "^7.0.3",
    "dotenv": "^8.2.0",
    "express": "^4.18.2",
    "lodash": "^4.17.20",
    "nodemailer-sendgrid": "^1.0.3",
    "payload": "^1.6.4",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.9",
    "copyfiles": "^2.4.1",
    "html-webpack-plugin": "^4.5.0",
    "nodemon": "^2.0.20",
    "ts-loader": "^9.4.2",
    "ts-node": "^9.1.1",
    "typescript": "^5.0.0-dev.20230215",
    "webpack": "^5.75.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^3.11.0"
  },
  "engines": {
    "node": "18.13.0",
    "yarn": "=1.22.19"
  }
}
